xrdb ~/.Xresources
xsettingsd &

[[ -f ~/.fehbg ]] && . ~/.fehbg

udiskie &

unclutter -root -idle 2 &
picom &

setxkbmap -option ctrl:nocaps -model pc105 -layout "fr" -variant "bepo"
xmodmap ~/.Xmodmap
xcape -e 'Control_L=Escape'
xinput set-prop "DLL0704:01 06CB:76AE Touchpad" "libinput Tapping Enabled" 1

# vim:ft=sh
